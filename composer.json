{
  "name": "adelowo/cfar",
  "type": "library",
  "description": "Using Controllers with Aura.Router",
  "keywords": [
    "controller for aura router",
    "aura router"
  ],
  "license": "MIT",
  "homepage": "https://github.com/adelowo/cfar",
  "authors": [
    {
      "name": "Lanre Adelowo",
      "email": "me@adelowolanre.com",
      "homepage": "http://adelowolanre.com",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "~5.5|~7.0",
    "aura/router": "^3.0",
    "container-interop/container-interop": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "Adelowo\\Cfar\\": "cfar/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload-dev": {
    "psr-4": {
      "Adelowo\\Cfar\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "suggest": {
    "pimple/pimple": "A lightweight container that works (~3.0)"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
